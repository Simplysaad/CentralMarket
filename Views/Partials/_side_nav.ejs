      <i class="fa fa-box"> </i>
      <a class="nav-link" href="/account/history">History</a>
    </li>
    <li class="nav-item p-2 d-flex gap-2">
      <i class="fa fa-money"> </i>
      <a class="nav-link" href="/account/payments">Payments</a>
    </li>
  </ul>
</div>

<div id="categories" class="my-4 border-bottom">
  <div class="text-heading">
    <h3 class="fs-5 fw-light text-muted text-capitalize p-2">categories</h3>
  </div>
  
  <% if(locals.categories){%>
    <% locals.categories.forEach((category)=>{%>
      
      <ul class="list-unstyled d-flex flex-column gap-2 side-nav-menu">
        <li class="nav-item p-2 d-flex gap-2">
          <i class="fa fa-tag"> </i>
          <a class="nav-link text-capitalize" href="/category/<%= category %>">
            <%= category %>
          </a>
        </li>
      </ul>
      <% })%>
        <% }%>
</div>

